{
    "name": "flux/boxidiomes",
    "description": "Box Idiomes Symfony 2 Website",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "ancarebeca/full-calendar-bundle": "^2.0",
        "apoutchika/loremipsum-bundle": "^1.0",
        "beberlei/DoctrineExtensions": "^1.0",
        "components/font-awesome": "^4.0",
        "components/jquery": "^2.0",
        "digitick/sepa-xml": "^1.3",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/migrations": "^1.0",
        "doctrine/orm": "^2.4.8",
        "drewm/mailchimp-api": "^2.4",
        "egeloen/ckeditor-bundle": "^4.0",
        "egulias/email-validator": "^1.2",
        "excelwebzone/recaptcha-bundle": "^1.4",
        "friendsofsymfony/jsrouting-bundle": "^1.6",
        "hampe/zurb-ink-bundle": "^2.2",
        "hautelook/alice-bundle": "^1.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "ircmaxell/password-compat": "~1.0",
        "it-blaster/color-picker-type-bundle": "^1.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "liip/functional-test-bundle": "^1.0",
        "liip/imagine-bundle": "^1.0",
        "php": ">=7.1.0",
        "presta/sitemap-bundle": "^1.3",
        "saad-tazi/g-chart-bundle": "^1.5",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/core-bundle": "3.16.1",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/user-bundle": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.7",
        "symfony/monolog-bundle": "~2.4",
        "symfony/symfony": "2.8.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "twbs/bootstrap": "^3.0",
        "twig/extensions": "^1.3",
        "vich/uploader-bundle": "^1.5",
        "whiteoctober/tcpdf-bundle": "^1.0"
    },
    "require-dev": {
        "cypresslab/gitelephant": "3.0.0",
        "cypresslab/gitelephant-bundle": "^0.0.6",
        "sensio/generator-bundle": "~3.0",
        "phpunit/phpunit": "^5.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
